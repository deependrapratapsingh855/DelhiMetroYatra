<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context=".Parking">

    <Button
        android:layout_width="200dp"
        android:layout_height="65dp"
        android:text="Search"
        android:id="@+id/buttonp"
        android:layout_marginTop="62dp"
        android:background="@drawable/custom_button"
        android:textSize="20dp"
        android:layout_below="@+id/autoCompleteTextView2"
        android:layout_centerHorizontal="true" />


    <AutoCompleteTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/autoCompleteTextView2"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:hint="Station"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:layout_alignTop="@+id/autoCompleteTextView2"
        android:layout_alignBottom="@id/autoCompleteTextView2"
        android:layout_alignRight="@id/autoCompleteTextView2"
        android:src="@drawable/arrow"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/textView14"
        android:textColor="#ff0004"
        android:textSize="24dp"
        android:layout_below="@+id/buttonp"
        android:layout_alignLeft="@+id/autoCompleteTextView2"
        android:layout_alignStart="@+id/autoCompleteTextView2"

        android:layout_marginTop="20dp"
        android:gravity="center" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/textView15"
        android:layout_below="@+id/textView14"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp"
        android:textSize="20dp"
        android:layout_marginLeft="10dp" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/textView16"
        android:layout_below="@+id/textView15"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp"
        android:textSize="20dp"
        android:layout_marginLeft="10dp"/>

</RelativeLayout>